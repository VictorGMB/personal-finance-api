    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>cafee1e1-4f6d-41ee-85f6-5944a00a300e</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.5" />
